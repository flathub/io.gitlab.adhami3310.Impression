{
    "id": "io.gitlab.adhami3310.Impression",
    "runtime": "org.gnome.Platform",
    "runtime-version": "49",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command": "impression",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--system-talk-name=org.freedesktop.UDisks2",
        "--share=network"
    ],
    "build-options": {
        "append-path": "/usr/lib/sdk/rust-stable/bin",
        "env": {
            "CARGO_HOME": "/run/build/impression/cargo"
        }
    },
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name": "impression",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://gitlab.com/-/project/46498291/uploads/9b955758f9e525bc3f5b8b6be93b3b40/impression-3.5.2.tar.xz",
                    "sha256": "3661639006a4eb04643bb21458dd60cd3daaba8801e634eb2fb65e31a45c2a34"
                }
            ]
        }
    ]
}
