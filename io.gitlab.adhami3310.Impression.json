{
    "id": "io.gitlab.adhami3310.Impression",
    "runtime": "org.gnome.Platform",
    "runtime-version": "49",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command": "impression",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--system-talk-name=org.freedesktop.UDisks2",
        "--share=network"
    ],
    "build-options": {
        "append-path": "/usr/lib/sdk/rust-stable/bin",
        "env": {
            "CARGO_HOME": "/run/build/impression/cargo"
        }
    },
    "modules": [
        {
            "name": "impression",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://gitlab.com/-/project/46498291/uploads/cdb991852d795587bc6ad75916c22eaf/impression-3.6.0.tar.xz",
                    "sha256": "162c7a5dbc5511e2a19ccebc2fa68625ff61e703225af7c09b0bf17a3981976e"
                }
            ]
        }
    ]
}
