{
    "id": "io.gitlab.adhami3310.Impression",
    "runtime": "org.gnome.Platform",
    "runtime-version": "49",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command": "impression",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--system-talk-name=org.freedesktop.UDisks2",
        "--share=network"
    ],
    "build-options": {
        "append-path": "/usr/lib/sdk/rust-stable/bin",
        "env": {
            "CARGO_HOME": "/run/build/impression/cargo"
        }
    },
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name": "impression",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://gitlab.com/-/project/46498291/uploads/d1d9a12bd5795d4672ad6b4c08a07777/impression-3.5.4.tar.xz",
                    "sha256": "3ef92f85b1670b8e4b483dcf7f70556d67635d899efb341eb1c690241d02df9a"
                }
            ]
        }
    ]
}
